@model ProjectCinema.Models.Hall

@{
    ViewBag.Title = "Hall Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Hall Delete</h2>


@using (Html.BeginForm("Delete", "Admin", FormMethod.Post, new { id = "popupForm" }))
{
    @Html.HiddenFor(a => a.IDHall)
    <div class="form-group">
        <label>IDHall</label>
        <p>@Model.IDHall</p>
    </div>

    <div class="form-group">
        <label>NumberOfseat</label>
        <p>@Model.NumberOfseat</p>
    </div>


    <div>
        <input type="submit" value="Delete" />
    </div>
}




